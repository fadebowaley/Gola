<script>


  document.querySelector('#submit-message').addEventListener('click', async (e) => {
    e.preventDefault();

    // Retrieve form data
    const formData = {
        fullName: document.querySelector('#fullname').value,
        email: document.querySelector('#emailing').value,
        phone: document.querySelector('#phonenumber').value,
        subjectSpec: document.querySelector('#title-subject').value,
        subject: document.querySelector('#title-drop-down').value,
        message: document.querySelector('#main-message').value
    };

    try {
        // Send POST request
        const response = await fetch('/send-message', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify(formData)
        });

        const result = await response.json();

        if (response.ok) {
            // Success alert
            const alertBox = `<div class="alert alert-success alert-dismissible fade show" role="alert">
                <strong>Success!</strong> Your message was sent successfully.
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>`;

            // Check if alert container exists, and then add the alert
            const alertContainer = document.getElementById("alert");
            if (alertContainer) {
                alertContainer.innerHTML = alertBox;
                //reset the form
                document.querySelector('#contact-form-message').reset();


            } else {
                console.warn("Alert container not found.");
            }
        } else {
            // Error alert
            alert(`Error: ${result.error || 'Message could not be sent.'}`);
        }
    } catch (error) {
        console.error('Error submitting form:', error);
        alert('There was an issue sending your message. Please try again.');
    }
  });


  async function fetchBlogs() {
    try {
      const response = await fetch('/api/blogs');
      const blogs = await response.json();

      const blogContainer = document.getElementById('blog-articles');
      blogContainer.innerHTML = blogs.map(blog => `
                    <div
                      class="col-md-6 scroll-animation mb-2"
                      data-animation="fade_from_left">
                      <div class="pricing-table">
                        <div class="pricing-table-header">
                          <div
                            class="top d-flex justify-content-between align-items-start">
                            <p>${new Date(blog.date).toLocaleDateString()}</p>
                            <p class="text-right">
                              by ${blog.author}<br />
                            </p>
                          </div>
                          <h3>${blog.title}</h3>
                        </div>
                        <!-- <ul class="feature-lists">
                          <li>${blog.excerpt}</li>
                        </ul> -->
                        <a href="${blog.link}" target="_blank" class="theme-btn">Read More</a>
                      </div>
                    </div>
      `).join('');



    } catch (error) {
      console.error('Error fetching blogs:', error);
    }
  }

  // Fetch blogs on page load
  document.addEventListener('DOMContentLoaded', fetchBlogs);

</script>


